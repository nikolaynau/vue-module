{
  "name": "@vuemodule/vue",
  "version": "1.0.3",
  "type": "module",
  "description": "Plugin and Composition API utilities for @vuemodule/core.",
  "author": "Nikolay Naumenkov<https://github.com/nikolaynau>",
  "license": "MIT",
  "homepage": "https://github.com/nikolaynau/vue-module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolaynau/vue-module.git"
  },
  "bugs": {
    "url": "https://github.com/nikolaynau/vue-module/issues"
  },
  "keywords": [
    "vue",
    "module"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./builtin": {
      "types": "./dist/builtin/index.d.ts",
      "import": "./dist/builtin/builtin.es.js",
      "require": "./dist/builtin/builtin.umd.js"
    }
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && vite build -c vite.config.builtin.ts"
  },
  "peerDependencies": {
    "vue": ">=3.0.0",
    "vue-router": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "vue-router": {
      "optional": true
    }
  },
  "dependencies": {
    "@vuemodule/core": "workspace:*"
  },
  "devDependencies": {
    "vite-plugin-dts": "^4.5.3",
    "vite": "^6.2.0"
  }
}
